<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:drawable="@android:drawable/picture_frame"
        android:right="4dp"
        android:left="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="5dp"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/foregroundWhite">
            </solid>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>
</layer-list>